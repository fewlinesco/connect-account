{
  "name": "@connect-account/frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p $CONNECT_ACCOUNT_FRONTEND_PORT",
    "build": "next build",
    "lint": "eslint \"{src,pages}/**/*.{js,ts,jsx}\"",
    "start": "next start -pCONNECT_ACCOUNT_FRONTEND_PORT"
  },
  "dependencies": {
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "^2.0.8",
    "styled-components": "5.1.0"
  },
  "devDependencies": {
    "@fewlines/eslint-config": "2.0.1",
    "@types/node": "13.13.4",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.7",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.18.3",
    "file-loader": "^6.0.0",
    "prettier": "2.0.5",
    "typescript": "3.8.3",
    "url-loader": "^4.1.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "@fewlines/eslint-config/react",
      "@fewlines/eslint-config/react-typescript",
      "@fewlines/eslint-config/typescript"
    ],
    "rules": {
      "explicit-function-return-type": 0
    }
  }
}
