{
  "name": "connect-account",
  "version": "0.1.0",
  "author": "Fewlines",
  "private": true,
  "repository": "git@github.com:fewlinesco/connect-account.git",
  "cacheDirectories": [
    ".next/cache"
  ],
  "scripts": {
    "dev": "next dev -p $CONNECT_ACCOUNT_PORT",
    "build": "next build",
    "lint": "eslint \"{src,pages}/**/*.{js,ts,jsx,tsx}\"",
    "start": "next start -p $PORT",
    "test": "jest --testPathIgnorePatterns=e2e --runInBand",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "db:put-user": "ts-node bin/dynamodb/put-user.ts",
    "db:create-users-table": "ts-node bin/dynamodb/create-users-table.ts",
    "db:delete-users-table": "ts-node bin/dynamodb/delete-users-table.ts",
    "db:get-user-from-sub": "ts-node bin/dynamodb/get-user-from-sub.ts",
    "e2e:add-heroku-redirect-uri": "node bin/e2e/add-heroku-redirect-uri-to-connect.mjs",
    "e2e:remove-heroku-redirect-uri": "node bin/e2e/remove-heroku-redirect-uri-from-connect.mjs",
    "e2e:create-test-user": "ts-node bin/e2e/create-test-user.ts",
    "e2e:remove-test-user": "ts-node bin/e2e/remove-test-user.ts",
    "e2e:create-status-check": "ts-node bin/e2e/create-e2e-status-check.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.11.0",
    "@aws-sdk/util-dynamodb": "3.11.0",
    "@fewlines/connect-client": "0.4.1",
    "@fewlines/connect-management": "0.6.2",
    "@fwl/logging": "0.1.1",
    "@fwl/tracing": "0.9.0",
    "@fwl/web": "0.11.3",
    "@hapi/iron": "6.0.0",
    "@react-aria/button": "3.3.1",
    "@react-aria/link": "3.1.2",
    "@react-aria/ssr": "3.0.1",
    "@react-aria/textfield": "3.2.2",
    "@sentry/browser": "6.2.5",
    "@sentry/node": "6.2.5",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "cross-fetch": "3.1.4",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.3",
    "js-cookie": "2.2.1",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-phone-number-input": "3.1.18",
    "styled-components": "5.2.3",
    "swr": "0.5.5",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@fewlines/eslint-config": "3.1.2",
    "@storybook/addon-actions": "6.2.5",
    "@storybook/addon-essentials": "6.2.5",
    "@storybook/addon-links": "6.2.5",
    "@storybook/react": "6.2.5",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "@types/jest": "26.0.22",
    "@types/js-cookie": "2.2.6",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-phone-number-input": "3.0.6",
    "@types/styled-components": "5.1.9",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "file-loader": "6.2.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "jest-styled-components": "7.0.3",
    "prettier": "2.2.1",
    "taiko": "1.2.3",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.2.4",
    "url-loader": "4.1.1"
  },
  "engines": {
    "node": ">=14.8",
    "yarn": ">=1.22.4"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {},
              "transform-runtime": {},
              "styled-jsx": {},
              "class-properties": {}
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "development": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {},
              "transform-runtime": {},
              "styled-jsx": {},
              "class-properties": {}
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ],
          "@babel/plugin-transform-react-jsx"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@fewlines/eslint-config/node",
      "@fewlines/eslint-config/react",
      "@fewlines/eslint-config/typescript",
      "@fewlines/eslint-config/react-typescript"
    ],
    "overrides": [
      {
        "files": [
          "*.js",
          "*.mjs"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      }
    ],
    "rules": {
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/explicit-function-return-type": [
        "error",
        {
          "allowExpressions": true
        }
      ],
      "import/order": [
        "error",
        {
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": false
          },
          "newlines-between": "always",
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ],
          "pathGroups": [
            {
              "pattern": "@src/**",
              "group": "internal"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "verbose": true,
    "unmockedModulePathPatterns": [
      "node_modules/react/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules",
      "config"
    ],
    "moduleNameMapper": {
      "\\.(png|svg|jpg|gif|eot|ttf|woff|woff2)$": "<rootDir>/tests/config/import-asset-fix.ts",
      "\\.(css|less)$": "<rootDir>/tests/config/import-asset-fix.ts",
      "@src/(.*)": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
